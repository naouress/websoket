<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="230.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="webSocket.msgController">
   <children>
      <ToolBar fx:id="bar" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <items>
            <StackPane>
               <children>
                  <Circle fill="#8999a8" radius="16.0" stroke="BLACK" strokeType="INSIDE" />
                  <Label fx:id="user" text="L">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
            <Label fx:id="txt" text="" />
         </items>
      </ToolBar>
   </children>
</AnchorPane>
